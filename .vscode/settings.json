{
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": true
    },
    "eslint.validate": ["typescript"],
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      
    },
    "prettier.trailingComma": "none",
    "prettier.useEditorConfig": false
  }
  